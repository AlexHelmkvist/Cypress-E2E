{
  "name": "cypress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:all": "node run-tests.js",
    "test": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:all-legacy": "npm run cy:run:chrome && npm run cy:run:firefox && npm run cy:run:edge",
    "cy:run:all": "node run-tests.js",
    "merge-reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && marge cypress/reports/merged-report.json -f report -o cypress/reports",
    "clean-reports": "powershell -Command \"Remove-Item -Path 'cypress/reports/*.json' -Force -ErrorAction SilentlyContinue; Remove-Item -Path 'cypress/reports/merged-report.json' -Force -ErrorAction SilentlyContinue\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cypress": "^0.1.6",
    "chromedriver": "^131.0.3",
    "cypress": "^13.16.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^2.0.1",
    "geckodriver": "^5.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "selenium-webdriver": "^4.27.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "cypress": {
    "reporter": "mochawesome",
    "reporterOptions": {
      "reportDir": "cypress/reports",
      "overwrite": false,
      "html": true,
      "json": true
    }
  }
}